<div class="ma__error-list js-error-list" tabindex="-1">
  <div class="ma__error-list__container">
    <div class="ma__error-list__header">
      <span class="ma__error-list__icon">
        {% include "@atoms/05-icons/svg-input-error.twig" %}
      </span>
      <span class="ma__error-list__title">
        {{ errorList.title }}
      </span>
    </div>
    <ul class="ma__error-list__messages">
      {% for link in errorList.links %}
        <li class="ma__error-list__message">
          <a class="ma__error-list__link js-error-list-link" href="#{{ link.inputId }}" aria-label="{{ link.info }}">{{ link.text }}</a>
        </li>
      {% endfor %}
    </ul>
  </div>
</div>